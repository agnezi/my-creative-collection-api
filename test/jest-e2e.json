{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/bcrypt-config/(.*)": "<rootDir>/../libs/bcrypt-config/src/$1",
    "@app/bcrypt-config": "<rootDir>/../libs/bcrypt-config/src"
  }
}